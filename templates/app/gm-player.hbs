<tr class="character-resume" data-character-id="{{character.id}}">
    <td>{{data.name}}</td>
    <td>{{character.name}}</td>
    <td data-resource="flashlights">{{character.data.data.attributes.flashlights.value}}</td>
    <td data-resource="smokes">{{character.data.data.attributes.smokes.value}}</td>
    <td data-resource="sanity">{{character.data.data.attributes.sanity.value}}</td>
    {{#if (isEnabled "MiscellaneousResource")}}<td data-resource="miscellaneous">{{character.data.data.attributes.miscellaneous.value}}</td>{{else}}<td data-resource="wealthDice">{{character.data.data.attributes.wealthDice.value}}</td>{{/if}}</td>
    {{#if (isEnabled "HitDiceResource")}}<td data-resource="hitdice">{{character.data.data.attributes.hitDice.value}}</td>{{else}}<td data-resource="hp">{{character.data.data.hp.value}}</td>{{/if}}</td>
    {{#if (isEnabled "Adrenaline")}}
    <td data-resource="adrenaline">
        <img class="adrenaline-img" src="{{getAdrenalineImage character.data.data.attributes.adrenaline1.value}}"><img class="adrenaline-img" src="{{getAdrenalineImage character.data.data.attributes.adrenaline2.value}}">
    </td>
    {{/if}}
</tr>
